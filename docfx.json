{
  "build": {    
    "content": [
      {
        "files": [
          "**.md",
          "**.yml"
        ],
        "group": "v1.0",
        "src": "concepts",
        "dest": "concepts",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**.md",
          "**.yml"
        ],
        "group": "beta",
        "src": "concepts",
        "dest": "concepts",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "api/**.md",
          "resources/**.md",
          "**.yml"
        ],
        "src": "api-reference/v1.0/",        
        "group": "v1.0",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },      
      {
       "files": [
          "api/**.md",
          "resources/**.md",
          "**.yml"
        ],
        "src": "api-reference/beta/",       
        "group": "beta",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "group": "v1.0",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "group": "beta",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "groups": {
      "v1.0": {
        "dest": "v1temp",
        "moniker_range": ">= graph-rest-1.0"
      },
      "beta": {
        "dest": "betatemp",
        "moniker_range": "graph-rest-beta"
      }     
    },    
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": { 
      "feedback_system": "GitHub",
      "feedback_github_repo": "microsoftgraph/microsoft-graph-docs",
      "feedback_product_url": "https://officespdev.uservoice.com/forums/224641-feature-requests-and-feedback/category/101632-microsoft-graph-o365-rest-apis",
      "breadcrumb_path": "/graph/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "author": "jthake-msft",
      "ms.author": "jthake-msft",
      "ms.date": "06/11/2018"
    },
    "fileMetadata": {   
      "monikerRange": {
        "api-reference/1.0/**": "graph-rest-1.0",
        "api-reference/beta/**": "graph-rest-beta"
      },
      "feedback_system": {},
      "ms.service": {
        "concepts/*.md": "microsoft-graph", 
        "api-reference/v1.0/api/*.md": "microsoft-graph",  
        "api-reference/beta/api/*.md": "microsoft-graph",
        "api-reference/v1.0/resources/*.md": "microsoft-graph",  
        "api-reference/beta/resources/*.md": "microsoft-graph"    
      },
      "ms.topic": {
        "concepts/*.md": "conceptual",  
        "api-reference/v1.0/api/*.md": "reference",  
        "api-reference/beta/api/*.md": "reference",
        "api-reference/v1.0/resources/*.md": "reference",  
        "api-reference/beta/resources/*.md": "reference" 
      }
    },
    "template": [],
    "dest": "microsoft-graph-docs",
    "markdownEngineName": "markdig"
  }
}
